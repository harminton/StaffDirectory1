@model IEnumerable<StaffDirectory.Models.TimeTable>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstPeriod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SecondPeriod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThirdPeriod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FourthPeriod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FifthPeriod)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Staff)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstPeriod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SecondPeriod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ThirdPeriod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FourthPeriod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FifthPeriod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Staff.StaffID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.AcNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TimeTableID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TimeTableID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TimeTableID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
